{
  "name": "docs-preview",
  "displayName": "docs-preview",
  "description": "Docs Markdown Preview Extension",
  "version": "0.3.18",
  "publisher": "docsmsft",
  "icon": "images/docs-logo-ms.png",
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-docs-authoring/issues"
  },
  "homepage": "https://github.com/Microsoft/vscode-docs-authoring/tree/master/docs-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-docs-authoring.git"
  },
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "markdown",
    "commonmark",
    "dfm",
    "docfx"
  ],
  "activationEvents": [
    "onCommand:docs.showPreview",
    "onCommand:docs.showPreviewToSide",
    "onLanguage:markdown"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "docs.showPreviewToSide",
        "title": "Docs: Preview",
        "when": "editorTextFocus"
      },
      {
        "command": "docs.showPreview",
        "title": "Docs: Preview in current tab",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/dfm.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Markdown",
      "order": 20,
      "properties": {
        "markdown.styles": {
          "type": "array",
          "default": [],
          "description": "%markdown.styles.dec%",
          "scope": "resource"
        },
        "markdown.preview.breaks": {
          "type": "boolean",
          "default": false,
          "description": "%markdown.preview.breaks.desc%",
          "scope": "resource"
        },
        "markdown.preview.linkify": {
          "type": "boolean",
          "default": true,
          "description": "%markdown.preview.linkify%",
          "scope": "resource"
        },
        "markdown.preview.fontFamily": {
          "type": "string",
          "default": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
          "description": "%markdown.preview.fontFamily.desc%",
          "scope": "resource"
        },
        "markdown.preview.fontSize": {
          "type": "number",
          "default": 14,
          "description": "%markdown.preview.fontSize.desc%",
          "scope": "resource"
        },
        "markdown.preview.lineHeight": {
          "type": "number",
          "default": 1.6,
          "description": "%markdown.preview.lineHeight.desc%",
          "scope": "resource"
        },
        "markdown.preview.scrollPreviewWithEditor": {
          "type": "boolean",
          "default": true,
          "description": "%markdown.preview.scrollPreviewWithEditor.desc%",
          "scope": "resource"
        },
        "markdown.preview.scrollPreviewWithEditorSelection": {
          "type": "boolean",
          "default": true,
          "description": "%markdown.preview.scrollPreviewWithEditorSelection.desc%",
          "deprecationMessage": "%markdown.preview.scrollPreviewWithEditorSelection.deprecationMessage%"
        },
        "markdown.preview.markEditorSelection": {
          "type": "boolean",
          "default": true,
          "description": "%markdown.preview.markEditorSelection.desc%",
          "scope": "resource"
        },
        "markdown.preview.scrollEditorWithPreview": {
          "type": "boolean",
          "default": true,
          "description": "%markdown.preview.scrollEditorWithPreview.desc%",
          "scope": "resource"
        },
        "markdown.preview.doubleClickToSwitchToEditor": {
          "type": "boolean",
          "default": true,
          "description": "%markdown.preview.doubleClickToSwitchToEditor.desc%",
          "scope": "resource"
        },
        "markdown.preview.openMarkdownLinks": {
          "type": "string",
          "default": "inPreview",
          "description": "%configuration.markdown.preview.openMarkdownLinks.description%",
          "scope": "resource",
          "enum": [
            "inPreview",
            "inEditor"
          ],
          "enumDescriptions": [
            "%configuration.markdown.preview.openMarkdownLinks.inPreview%",
            "%configuration.markdown.preview.openMarkdownLinks.inEditor%"
          ]
        },
        "markdown.trace": {
          "type": "string",
          "enum": [
            "off",
            "verbose"
          ],
          "default": "off",
          "description": "%markdown.trace.desc%",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^5.2.0",
    "typescript": "^2.5.2",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.8",
    "axios": "^0.17.1",
    "cheerio": "^1.0.0-rc.2",
    "child_process": "^1.0.2",
    "command-exists": "^1.2.7",
    "file-url": "^2.0.2",
    "highlightjs": "^9.10.0",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "jquery": "^3.2.1",
    "mkdirp": "^0.5.1",
    "tmp": "0.0.33",
    "vscode-languageclient": "^3.4.2",
    "yauzl": "^2.9.1"
  },
  "runtimeDependencies": [
    {
      "description": "docs-previewService for .NET Core",
      "url": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/netcore.zip",
      "fallbackUrl": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/netcore.zip",
      "installPath": ".markdocs",
      "platforms": [
        "netcore"
      ],
      "architectures": [
        "netcore"
      ],
      "installTestPath": "./.markdocs/DocsPreviewService.dll"
    },
    {
      "description": "docs-previewService for Windows (x86)",
      "url": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/win32_x86.zip",
      "fallbackUrl": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/win32_x86.zip",
      "installPath": ".markdocs",
      "platforms": [
        "win32"
      ],
      "architectures": [
        "x86"
      ],
      "installTestPath": "./.markdocs/DocsPreviewService.exe"
    },
    {
      "description": "docs-previewService for Windows (x64)",
      "url": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/win32_x64.zip",
      "fallbackUrl": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/win32_x64.zip",
      "installPath": ".markdocs",
      "platforms": [
        "win32"
      ],
      "architectures": [
        "x86_64"
      ],
      "installTestPath": "./.markdocs/DocsPreviewService.exe"
    },
    {
      "description": "docs-previewService for macOS (x64)",
      "url": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/darwin_x64.zip",
      "fallbackUrl": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/darwin_x64.zip",
      "installPath": ".markdocs",
      "platforms": [
        "darwin"
      ],
      "binaries": [
        "./DocsPreviewService"
      ],
      "installTestPath": "./.markdocs/DocsPreviewService"
    },
    {
      "description": "docs-previewService for Linux (x64)",
      "url": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/linux_x64.zip",
      "fallbackUrl": "https://github.com/qinezh/markdocs-server/releases/download/v1.0.44/linux_x64.zip",
      "installPath": ".markdocs",
      "platforms": [
        "linux"
      ],
      "architectures": [
        "x86_64"
      ],
      "binaries": [
        "./DocsPreviewService"
      ],
      "installTestPath": "./.markdocs/DocsPreviewService"
    }
  ]
}